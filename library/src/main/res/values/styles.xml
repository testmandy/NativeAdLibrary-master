<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="foodRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratings_bar</item>
        <item name="android:minHeight">12dip</item>
        <item name="android:maxHeight">12dip</item>
    </style>

    <style name="ContactsListItem_Photo">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_width">40dip</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:contentDescription">@string/app_name</item>
    </style>

    <style name="sky_btn_yellow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/traffic_bg</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
    </style>
</resources>